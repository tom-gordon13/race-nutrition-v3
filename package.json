{
    "name": "race-nutrition",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "cd ui && npm run dev",
        "dev:api": "tsx watch api/src/server.ts",
        "build": "prisma generate && prisma migrate deploy && cd ui && npm run build && cd .. && cp -r ui/dist dist",
        "lint": "cd ui && npm run lint",
        "preview": "cd ui && npm run preview",
        "install:ui": "cd ui && npm install",
        "clean": "rm -rf dist",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@auth0/auth0-react": "^2.4.0",
        "@prisma/client": "^6.18.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.14",
        "prisma": "^6.18.0",
        "tsx": "^4.20.6",
        "typescript": "^5.3.3"
    }
}
