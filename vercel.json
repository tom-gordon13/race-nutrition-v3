{
  "buildCommand": "cd ui && npm run build",
  "outputDirectory": "ui/dist",
  "installCommand": "npm install && cd api && npm install && cd .. && npm run install:ui && npx prisma generate",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 10
    }
  }
}
